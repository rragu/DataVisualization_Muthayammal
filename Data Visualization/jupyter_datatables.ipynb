       " */\n",
       "\n",
       "define('dt-graph-objects', ['moment', 'chartjs'], function (moment, chartjs) {\n",
       "  const d3 = Object.assign({}, require('d3'), require('d3-array')) // extend the d3 with d3-array\n",
       "\n",
       "  Chart.defaults.scale.gridLines.display = false\n",
       "\n",
