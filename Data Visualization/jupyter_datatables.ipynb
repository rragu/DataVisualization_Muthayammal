       "    console.debug('Line plot data: ', data, 'labels: ', labels)\n",
       "\n",
       "    let canvas = $('<canvas>')\n",
       "      .attr('width', layout.width)\n",
       "      .css('margin', layout.margin)\n",
       "\n",
       "    let ctx = canvas.get(0).getContext('2d')\n",
       "\n",
       "    let chart = new Chart(ctx, {\n",
